{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "immersiveui",
    "version": "${version}",
    "metadata": {
      "name": "ImmersiveUI",
      "description": "Makes your UI flashy :D",
      "contributors": {
        "Me!": "Octo-Studios"
      }
    },
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "it.hurts.octostudios.quilt.ImmersiveUIQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.20.1"
      }
    ]
  },
  "mixin": [
    "immersiveui.mixins.json",
    "immersiveui-common.mixins.json"
  ]
}
